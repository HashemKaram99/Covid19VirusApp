<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".jsonActivity">

    <ImageButton
        android:id="@+id/imageButton_json"
        android:layout_width="351dp"
        android:layout_height="255dp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/image1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="28dp" />

    <TextView
        android:id="@+id/cases"
        android:layout_width="258dp"
        android:layout_height="67dp"
        android:background="@color/colorPrimaryDark"
        android:gravity="center"
        android:padding="15dp"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/textView22"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="88dp" />

    <TextView
        android:id="@+id/desths"
        android:layout_width="258dp"
        android:layout_height="67dp"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:padding="15dp"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/textView32"
        app:layout_constraintTop_toBottomOf="@+id/textView22"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="88dp" />

    <TextView
        android:id="@+id/recovered"
        android:layout_width="258dp"
        android:layout_height="67dp"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:padding="15dp"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="88dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="186dp"
        android:layout_height="36dp"
        android:text="The Cases"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/cases"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_json"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="49dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="186dp"
        android:layout_height="36dp"
        android:text="The Desths"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/desths"
        app:layout_constraintTop_toBottomOf="@+id/cases"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="49dp" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="186dp"
        android:layout_height="36dp"
        android:text="The Recovered"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/recovered"
        app:layout_constraintTop_toBottomOf="@+id/desths"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="49dp" />
</androidx.constraintlayout.widget.ConstraintLayout>